using UnityEngine;
using System.Collections;

public class JumpUpPickup : MonoBehaviour {

	// Use this for initialization
	void Start () {
	
	}

    void OnTriggerEnter2D(Collider2D other)
    {
        if (other.name == "Player")
        {
            Player.JumpPowerUp = 1;
            Destroy(gameObject);
        }
    }
}
